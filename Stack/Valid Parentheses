class Solution {
public:
    bool isValid(string s) {
        stack<char> st;
        for(int i=0;i<s.size();i++)
        {
            if(st.empty())
            {
                st.push(s[i]);
                continue;
            }
            if((s[i]==')' and st.top()=='(') || (s[i]=='}' and st.top()=='{') || (s[i]==']' and st.top()=='['))
            {
                st.pop();
            }
            else
            {
                st.push(s[i]);
            }
            }
        if(st.empty())
        return true;
        return false;
    }
};
